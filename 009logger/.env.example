# Logger Configuration
LOG_FILE=./logs/simple-logger.log
LOG_LEVEL=info
LOG_FORMAT=text
LOG_OUTPUT=both

# File Rotation Settings
MAX_SIZE=100
MAX_BACKUPS=3
MAX_AGE=7
COMPRESS=true

# Server Configuration
SERVER_PORT=8080
SERVER_HOST=localhost

# Timestamp Format
TIMESTAMP_FORMAT=2006-01-02 15:04:05
TIMEZONE=UTC